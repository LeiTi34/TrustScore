{
    "compilerOptions": {
      "jsx": "react-jsx", // Enable JSX for React  
      "target": "ES6",
      "module": "ESNext",
      "moduleResolution": "Node",
      "strict": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    }
  }
  